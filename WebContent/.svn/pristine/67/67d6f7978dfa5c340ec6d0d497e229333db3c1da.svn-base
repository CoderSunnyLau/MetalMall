.search_box{
	width:100%;
	height:100px;
}
.search_row{
	width:100%;
	height:50px;
	margin-left:-10px;
}
.search_row div{
	float:left;
	width:25%;
}
.search_row div span{
	width:25%;
	display:inline-block;
	*display:inline;
	*zoom:1;
	text-align:center;
	font-size:14px;
	line-height:30px;
}
.search_row div input, .search_row div select{
	width:70%;
	margin-left:2%;
	height:30px;
	line-height:30px;
	border-radius:5px;
	border:1px solid #CCC;
	padding:0;
}
.search_row div select{
	margin-left:1%;
}
.search_box .search_price{
	width:32%;
}
.search_box .search_price input{
	width:34%;
}
.search_box .search_price i{
	display:inline-block;
	*display:inline;
	*zoom:1;
	width:7%;
	text-align:right;
}
.search_box .search_price span{
	width:19.5%;
}
.search_box button{
	float:right;
	margin-right:10px;
	width:10%;
	background-color:#E33;
}
.pdts_box{
	width:100%;
}
.pdts_box .rank{
	width:100%;
	height:26px;
	background-color:#EEE;
	text-align:right;
	border-bottom:1px solid #CCC;
}
.rank .rank_item{
	display:inline-block;
	*display:inline;
	*zoom:1;
	padding-left:20px;
	margin:0 20px;
	line-height:26px;
}
.pdts{
	width: 100%;
}
.pdt{
	position: relative;
	width: 100%;
	margin-bottom:3px;
	border-bottom:1px solid #DDD;
	background-color:#F8F8F8;
	overflow:hidden;
	padding:10px 0;
}
.pdt .pdt_l{
	float: left;
	width: 14%;
	text-align:center;
}
.pdt_l img{
	width: 110px;
	height: 115px;
	margin:0 5px;
}
.pdt .pdt_c{
	float: left;
	width: 68%;
}
.pdt_title{
	width:100%;
	height:35px;
	overflow:hidden;
}
.pdt_name{
	display:block;
	float:left;
	max-width:600px;
	font-size:18px;
	line-height:40px;
	margin-right:10px;
}
.company_name{
	line-height:45px;
}
.pdt_info{
	width:100%;
}
.pdt_state{
	float:left;
	width:33%;
}
.pdt_price{
	height:45px;
	line-height:45px;
	color:#E33;
	font-weight:600;
}
.pdt_price b{
	height:40px;
	font-size:30px;
	color:#E33;
}
.pdt_stock{
	font-size:12px;
	line-height:18px;
	height:18px;
	text-align:center;
	margin-top:5px;
}
.pdt_stock span{
	display:block;
	float:left;
	width:60px;
	border:1px solid #DDD;
	border-right:none;
	color:#999;
}
.pdt_stock i{
	display:block;
	float:left;
	width:90px;
	background-color:#F90;
	color:#FFF;
}
.pdt_info .pdt_detail{
	float:right;
	width:66%;
	margin-top:15px;
}
.pdt_dtl_cnt span{
	display:block;
	float:left;
	width:49%;
	height:30px;
	line-height:30px;
}
.pdt .pdt_r{
	float: left;
	width: 15.5%;
	height:115px;
	margin-right:15px;
}
.pdt_r .date{
	float:right;
	text-align:center;
	width:110px;
	height:45px;
	line-height:55px;
	clear:both;
}
.pdt_r button{
	width:110px;
	height:35px;
	font-size:16px;
	float:right;
	margin-top:20px;
	clear:both;
}
.pdt_name, .pdt_dtl_cnt span{
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.page_box{
	height:42px;
	line-height:40px;
	float:right;
	overflow:hidden;
	margin:10px 0;
}
.page_box .near_page, .page_box .change_page, .pages .page, .page_box .pages{
	display:block;
	float:left;
	font-size:16px;
}
 .page_btn, .pages .page{
	height:40px;
	border:1px solid #DDD;
	background-color:#FFF;
	color:#666;
	border-radius:0;
	margin:0 8px;
	padding:0 5px;
	text-align:center;
	cursor:pointer;
 }
 .pages .page{
 	width:30px;
 	margin:0 3px;
 }
 .change_page{
 	height:42px;
 }
 .change_page *{
 	display:inline-block;
 	*display:inline;
 	*zoom:1;
	font-size:16px;
 }
 .change_page input{
 	width:50px;
 	height:40px;
 	margin:0 5px;
 	padding:0 5px;
 }